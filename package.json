    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.7.0",
    "eslint-plugin-mocha": "^2.1.0",
    "mocha": "^2.4.5",
