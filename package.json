{
  "name": "restify-resourcify",
  "version": "1.0.5",
  "description": "Provides base class and ES2016 decorators to declare restify routes.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "build": "babel src --out-dir lib --source-maps",
